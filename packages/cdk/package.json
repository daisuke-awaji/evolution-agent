{
  "name": "@evolution-agent/cdk",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "lint": "eslint bin lib config",
    "lint:fix": "eslint bin lib config --fix",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all --require-approval never",
    "deploy:dev": "cdk deploy --all --require-approval never -c env=dev",
    "destroy": "cdk destroy --all --force",
    "destroy:dev": "cdk destroy --all --force -c env=dev"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.200.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.200.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
